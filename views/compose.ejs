<%- include("partials/header"); %>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label class="form-label" for="title">Title</label>
            <input class="form-control" type="text" id="title" name="titleInput">
            <label class="form-label" for="post">Post</label>
            <textarea class="form-control" rows="5" id="post" name="composeInput"></textarea>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>
    <%- include("partials/footer"); %>